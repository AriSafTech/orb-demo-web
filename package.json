{
  "name": "orb-demo-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "prepare": "husky",
    "api:generate:old": "openapi-generator-cli generate -g typescript-axios -i ./src/api/demo-api.json -o ./src/api/generated/ --skip-validate-spec",
    "api:gen": "pnpm api:gen:types && pnpm api:gen:doc",
    "api:gen:types": "openapi typegen ./src/api/demo-api.json > ./src/api/generated-api-types.d.ts",
    "api:gen:doc": "npx openapicmd read --strip openapi_client_axios --format json ./src/api/demo-api.json > ./src/api/generated-api-doc.json",
    "api:swagger": "openapi swagger-ui ./src/api/demo-api.json",
    "test": "vitest"
  },
  "engines": {
    "node": "18.x"
  },
  "engineStrict": true,
  "dependencies": {
    "@hookform/resolvers": "^3.3.4",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-toast": "^1.1.5",
    "@tanstack/query-core": "^5.36.0",
    "@tanstack/react-query": "^5.36.0",
    "@tanstack/react-table": "^8.16.0",
    "axios": "^1.6.8",
    "axios-auth-refresh": "^3.3.6",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.0",
    "immer": "^10.1.1",
    "lucide-react": "^0.378.0",
    "next": "14.2.3",
    "next-themes": "^0.3.0",
    "nextjs-toploader": "^1.6.12",
    "openapi-client-axios": "^7.5.4",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.51.4",
    "react-icons": "^5.2.1",
    "react-spinners": "^0.13.8",
    "sonner": "^1.4.41",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "^0.9.1",
    "zod": "^3.23.6",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@openapitools/openapi-generator-cli": "^2.13.4",
    "@tanstack/eslint-plugin-query": "^5.35.6",
    "@tanstack/react-query-devtools": "^5.36.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.3",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "openapicmd": "^2.3.1",
    "postcss": "^8",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
